    <PackageReference Include="OsuParsers" Version="1.7.1" />
    <PackageReference Include="ProcessMemoryDataFinder" Version="0.8.5" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>
  <ItemGroup>
